#include <stdio.h>

int main()
{
    int m;
    printf("Enter the Mark : ");
    scanf("%d",&m);
    if(m<40)
    {
        printf("Grade F");
    }
    else if(m>=40&&m<55)
    {
        printf("Grade D");
    }
    else if(m>=55&&m<70)
    {
        printf("Grade C");
    }
    else if(m>=70&&m<85)
    {
        printf("Grade B");
    }
    else
    {
        printf("Grade A");
    }
}
